#Function to print FizzBuzz
def fizzbuzz():
    for i in range(1,51):  #set the range
        if i%3==0 and i%5==0:
            print("FizzBuzz")
        elif i%3==0:
            print("Fizz")
        elif i%5==0:
            print("Buzz")
        else:
            print(i)
#Call the Function
fizzbuzz()
